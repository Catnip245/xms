package healthManagementSystem;

import java.util.Scanner;

public class Age extends HmsMenu {
    private int age;

    public void setAge() {
        Scanner input = new Scanner(System.in);
        System.out.print("Input Age>>");
        age = input.nextInt();
    }
    
    public int getAge() {
        return age;
    }
    
    public void delete() {
        age = 0;
    }
}
